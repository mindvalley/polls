<div class="box">
  <div class="box-title">
    Results
  </div>
  <div class="box-content">
    <table class="table table-bordered table-striped" id="results">
      <thead>
        <th>Nick Name</th>
        <th>Points</th>
      </thead>
      <tbody>
        <% @results.lines.each do |result| %>
          <% entry = Entry.where(name: result[0]).first %>
          <tr>
            <td><%= entry.name %></td>
            <td><%= result[1] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

